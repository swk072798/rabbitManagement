<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nwafu.databaseoprations.dao.RabbitEliminateMapper">
	<resultMap id="BaseResultMap" type="com.nwafu.databaseoprations.entity.RabbitEliminate">
		<result column="rabbitNo" jdbcType="VARCHAR" property="rabbitNo"/>
		<result column="eliminateDate" jdbcType="TIMESTAMP" property="eliminateDate"/>
		<result column="rabbitHouseNo" jdbcType="VARCHAR" property="rabbitHouseNo"/>
		<result column="cageNo" jdbcType="VARCHAR" property="cageNo"/>
		<result column="eliminateReason" jdbcType="VARCHAR" property="eliminateReason"/>
	</resultMap>
	<insert id="insert" parameterType="java.util.List">
		insert into rabbit_eliminate(rabbitNo,eliminateDate,rabbitHouseNo,cageNo,eliminateReason) values
		<foreach collection="list" index="index" item="item" separator=",">(#{item.rabbitNo,jdbcType=VARCHAR}, 
			#{item.eliminateDate,jdbcType=TIMESTAMP},
			#{item.rabbitHouseNo,jdbcType=VARCHAR},
			#{item.cageNo,jdbcType=VARCHAR},
			#{item.eliminateReason,jdbcType=VARCHAR}
		)</foreach>
	</insert>

	<select id="selectAll" resultMap="BaseResultMap">
		select * from rabbit_eliminate
	</select>

	<select id="selectRabbitEliminateRecordByCondition" parameterType="java.lang.String" resultMap="BaseResultMap">
		select * from rabbit_eliminate where ${condition}=#{value}
	</select>

	<delete id="deleteById" parameterType="java.util.List">
		delete from rabbit_eliminate where rabbitNo in
		<foreach close=")" collection="list" item="item" open="(" separator=",">#{item}
		</foreach>
	</delete>
</mapper>
